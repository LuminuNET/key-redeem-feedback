{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDirs": ["src"],
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": false,
    "types": ["jest"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules/**"],
  "include": ["src/**/*.ts", "tests/**/*.ts"]
}
